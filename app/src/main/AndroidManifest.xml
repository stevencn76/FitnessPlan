<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="truestrength.fitnessplan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.DeclaimerActivity" />
        <activity android:name=".activity.CreatePlanActivity" />
        <activity android:name=".activity.PlanActivity" />
        <activity android:name=".activity.PlanListActivity" />
        <activity android:name=".activity.ExerciseListActivity" />
        <activity android:name=".activity.ExerciseActivity" />
        <activity android:name=".activity.AboutActivity" />
        <activity android:name=".activity.HelpActivity" />
        <activity android:name=".activity.DaysSummaryActivity" />
        <activity android:name=".activity.PlansSummaryActivity"></activity>
    </application>

</manifest>